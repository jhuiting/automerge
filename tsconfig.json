{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "dom",
      "es6",
      "scripthost"
    ],
    "module": "commonjs",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "newLine": "LF",
    "strict": false,
    "outDir": "dist/",
    "sourceMap": true,
    "stripInternal": true,
    "target": "es6"
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts"
  ]
}
